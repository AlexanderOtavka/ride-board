<%= form_with(model: location, local: true) do |form| %>
  <%= render 'shared/error_explanation', errors: location.errors %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="actions">
    <%= form.button class: "btn waves-effect waves-light" %>
  </div>
<% end %>
